<Page x:Class="WpfApp1.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Margin="20">
        <Label Content="Email"/>
        <TextBox x:Name="TxtEmail" Margin="0,10" />
        <Label Content="Password"/>
        <TextBox x:Name="TxtPassword" Margin="0,10" />
        <Button Content="Login" Click="BtnLogin_Click" Margin="0,10"/>
        <Button Content="Go to Register" Click="BtnToRegister_Click"/>
    </StackPanel>
</Page>
